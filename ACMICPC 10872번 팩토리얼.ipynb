   "source": [
    "def factorial(num):\n",
    "    if num == 0 :\n",
    "        return 1\n",
    "    elif num == 1 :\n",
    "        return 1\n",
    "    else :\n",
    "        return num * factorial(num - 1)"
   ]
  },
