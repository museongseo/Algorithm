   "source": [
    "def solution(scoville, K):\n",
    "    count = 0\n",
    "    while min(scoville) < K :\n",
    "        scoville.sort()\n",
    "        scoville.append(scoville.pop(0) + 2 * scoville.pop(0))\n",
    "        count += 1\n",
    "    return count"
   ]
