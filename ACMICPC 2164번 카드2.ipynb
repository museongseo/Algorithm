      "source": [
        "N = int(input())\n",
        "card = [i+1 for i in range(N)]\n",
        "while len(card) > 1:\n",
        "  if len(card) % 2:\n",
        "    temp = [card[-1]]\n",
        "    temp.extend(card[1::2])\n",
        "    card = temp\n",
        "  else:\n",
        "    card = card[1::2]\n",
        "print(card[0])"
      ],
