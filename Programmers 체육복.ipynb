   "source": [
    "def solution(n, lost, reserve):\n",
    "    answer = n - len(lost)\n",
    "    for number in reserve:\n",
    "        if abs(number - 1) in lost:\n",
    "            answer += 1\n",
    "            continue\n",
    "    return answer"
   ]
  }
