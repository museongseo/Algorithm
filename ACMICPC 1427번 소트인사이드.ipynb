   "source": [
    "num = int(input())\n",
    "sortedNum = 0\n",
    "List = []\n",
    "while (num > 0) :\n",
    "    List.append(num%10)\n",
    "    num -= num%10\n",
    "    num //= 10\n",
    "List = sorted(List)\n",
    "List.reverse()\n",
    "for number in List:\n",
    "    sortedNum *= 10\n",
    "    sortedNum += number\n",
    "print(sortedNum)"
   ]
  }
