    "def solution(name):\n",
    "    name = list(name)\n",
    "    answer = len(name) - 1\n",
    "    for i in range(len(name)):\n",
    "        answer += min(ord(name[i]) - 65, 91 - ord(name[i]))\n",
    "    return answer"
   ]
