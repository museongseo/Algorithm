   "source": [
    "N = int(input())\n",
    "list = []"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(N):\n",
    "    x, y = map(int, input().split())\n",
    "    list.append((x, y))\n",
    "    \n",
    "for j in list:\n",
    "    rank = 1\n",
    "    for k in list:\n",
    "        if j[0] < k[0] and j[1] < k[1]:\n",
    "            rank += 1\n",
    "    print(rank, end=\" \")"
   ]
  }
