   "source": [
    "def solution(people, limit):\n",
    "    people = sorted(people)\n",
    "    answer = 0\n",
    "    i = 0\n",
    "    j = len(people) - 1\n",
    "    while (i <= j):\n",
    "        if people[i] + people[j] <= limit :\n",
    "            i += 1\n",
    "        j -= 1\n",
    "        answer += 1\n",
    "    return answer"
   ]
  }
