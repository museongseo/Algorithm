    "def solution(clothes):\n",
    "    hash_table = dict()\n",
    "    for name, kind in clothes:\n",
    "        if kind not in hash_table:\n",
    "            hash_table[kind] = 0\n",
    "        hash_table[kind] += 1\n",
    "    \n",
    "    answer = 1\n",
    "    for num in hash_table.values():\n",
    "        answer *= num + 1\n",
    "    return answer - 1"
   ]
  }
