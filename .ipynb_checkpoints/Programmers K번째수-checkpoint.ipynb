    "def solution(array, commands):\n",
    "    answer = []\n",
    "    for command in commands:\n",
    "        i = command[0]\n",
    "        j = command[1]\n",
    "        k = command[2]\n",
    "        arr = sorted(array[i-1 : j])\n",
    "        answer.append(arr[k-1])\n",
    "    return answer"
   ]
  }
