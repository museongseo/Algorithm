   "source": [
    "N = int(input())\n",
    "count = 1\n",
    "num = N\n",
    "while(1) :\n",
    "    num1 = (num % 10) + (num // 10)\n",
    "    num2 = (num % 10) * 10 + (num1 % 10)\n",
    "    if (num2 == N):\n",
    "        print(count)\n",
    "        break\n",
    "    else :\n",
    "        num = num2\n",
    "        count += 1"
   ]
